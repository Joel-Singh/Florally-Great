{
  "name": "florally-great",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "parcel build bin/www.ts --target node",
    "prestart": "npm run build",
    "start": "npm run clearAndPopulateDB && node ./dist/www.js",
    "start:dev": "nodemon --ext js,ts,css,pug,json --exec npm start",
    "serverstart": "DEBUG=express-locallibrary-tutorial:* npm run devstart",
    "test": "npm run typecheck && jest",
    "test:watch": "npm run test -- --watch",
    "typecheck": "tsc --noEmit",
    "debug": "node --inspect-brk --import tsx bin/www",
    "test:debug": "node --inspect-brk --import tsx node_modules/.bin/jest --run-in-band",
    "inspect": "node inspect localhost:9229",
    "formatAll": "npx prettier --write .",
    "clearAndPopulateDB": "tsx bin/clearAndPopulateDB.ts",
    "prepare": "git config core.hooksPath ./git-hooks"
  },
  "targets": {
    "node": {
      "context": "node"
    }
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "express-validator": "^7.0.1",
    "he": "^1.2.0",
    "http-errors": "~1.6.3",
    "mongoose": "^7.1.0",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@parcel/transformer-pug": "^2.11.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/he": "^1.2.0",
    "@types/jest": "^29.5.6",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.4",
    "babel-jest": "^29.6.1",
    "jest": "^29.7.0",
    "jsdom": "^23.2.0",
    "mongodb-memory-server": "^8.12.2",
    "nodemon": "^2.0.22",
    "parcel": "^2.11.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "typescript": "^5.1.6"
  }
}
